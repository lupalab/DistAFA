{
    "dataset": "mnist",
    "mask_type": "remaining",
    "image_shape": [28,28,1],
    "num_classes": 10,
    "batch_size": 64,
    "model": "acflow_classifier",
    "n_bits": 8,
    "data_constraint": 0.9,
    "residual_blocks": 2,
    "n_scale": 3,
    "base_dim": 32,
    "bottleneck": 1,
    "use_aff": 1,
    "skip": 1,
    "lambda_reg": 0.00005,
    "lambda_xent": 10.0,
    "lambda_nll": 1.0,
    "epochs": 500,
    "optimizer": "adam",
    "lr": 0.001,
    "lr_decay_steps": 100000,
    "lr_decay_rate": 0.5,
    "clip_gradient": 100,
    "exp_dir": "./exp/mnist/acflow_classifier",
    "summ_freq": 100,
    "eval_freq": 20,
    "eval_metrics": ["acc","nll","cond_sam"]
}