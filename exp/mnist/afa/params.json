{
    "env": "img",
    "dataset": "mnist",
    "ood_datasets": ["omniglot"],
    "mask_type": "remaining",
    "image_shape": [28,28,1],
    "num_classes": 10,
    "batch_size": 1,
    "budget": 25,
    "num_samples": 10,
    "model_cfg_file": "exp/mnist/acflow_classifier/params.json",
    "planner_cfg_file": "exp/mnist/group_afa/params.json",
    "detector_cfg_file": "detectors/PO3D/configs/mnist_v1.yml",
    "detector_log_path": "detectors/PO3D/exp/logs/mnist_v1",
    "detector_reward_coef": -1,
    "agent": "img_cls_hppo",
    "embed_type": "image",
    "embed_layers": [64,64],
    "actor_layers": [64,64],
    "critic_layers": [64,32],
    "gamma": 0.99,
    "lam": 0.95,
    "ratio_clip_range": 0.2,
    "ratio_clip_decay": 0,
    "n_iters": 1000,
    "warmup_iters": 20,
    "buffer_size": 10000,
    "buffer_batch_size": 500,
    "buffer_epochs": 5,
    "clean_buffer": 1,
    "lr_a": 0.001,
    "lr_c": 0.001,
    "clip_grad_norm": 1.0,
    "log_freq": 1,
    "eval_freq": 100,
    "save_freq": 1,
    "num_test_episodes": 20,
    "num_plot_episodes": 1,
    "num_detect_episodes": 20,
    "exp_dir": "./exp/mnist/afa"
}