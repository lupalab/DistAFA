{
    "dataset": "uci",
    "dfile": "./data/FICO/heloc_d23_c2.pkl",
    "mask_type": "remaining",
    "augment": 0,
    "dimension": 23,
    "num_classes": 2,
    "batch_size": 1024,
    "model": "actan_classifier",
    "transform": ["AF","TL","TL","TL","TL","AF"],
    "layer_cfg": ["ML","LR","CP2"],
    "rnncp_units": 256,
    "rnncp_layers": 2,
    "linear_hids": [256,256],
    "linear_rank": -1,
    "affine_hids": [256,256],
    "coupling_hids": [256,256],
    "prior": "autoreg",
    "n_components": 40,
    "prior_units": 256,
    "prior_layers": 2,
    "prior_hids": [256,256],
    "lambda_reg": 0.00005,
    "lambda_xent": 1.0,
    "lambda_nll": 1.0,
    "epochs": 500,
    "optimizer": "adam",
    "lr": 0.001,
    "lr_decay_steps": 10000,
    "lr_decay_rate": 0.5,
    "clip_gradient": 1.0,
    "exp_dir": "./exp/heloc/actan_classifier",
    "summ_freq": 100,
    "eval_freq": 20,
    "eval_metrics": ["acc","nll"]
}